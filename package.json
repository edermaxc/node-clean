{
  "name": "clean-node",
  "version": "1.0.0",
  "description": "exemplo de node com clean arch",
  "main": "index.js",
  "devDependencies": {
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.0",
    "standard": "^17.0.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "scripts": {
    "test": "jest --passWithNoTests --silent --colors --noStackTrace",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:unitall": "npm test -- --watchAll -c jest-unit-config.js",
    "test:integrationall": "npm test -- --watchAll -c jest-integration-config.js",
    "test:ci": "npm test -- --coverage",
    "test:staged": "npm test",
    "prepare": "husky install"
  },
  "dependencies": {
    "uuid": "^8.3.2",
    "validator": "^13.7.0"
  }
}
